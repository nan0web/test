{
	"name": "@nan0web/test",
	"version": "1.0.0",
	"description": "A test package with simple utilities for testing in node.js runtime",
	"main": "src/index.js",
	"type": "module",
	"types": "types/index.d.ts",
	"exports": {
		".": {
			"import": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./jsdom": {
			"import": "./src/jsdom.js",
			"types": "./types/jsdom.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "node --test",
		"test:coverage": "node --test --experimental-test-coverage --test-coverage-include=src/**/*.js"
	},
	"keywords": [
		"fetch",
		"mock",
		"testing",
		"http",
		"utility"
	],
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"files": [
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/yaro-rasta/nan0test.git"
	},
	"homepage": "https://github.com/yaro-rasta/nan0test#readme",
	"bugs": {
		"url": "https://github.com/yaro-rasta/nan0test/issues"
	},
	"dependencies": {
		"happy-dom": "^18.0.1"
	},
	"devDependencies": {
		"@nan0web/db": "workspace:*",
		"@nan0web/db-fs": "workspace:*",
		"@nan0web/types": "workspace:*"
	},
	"peerDependencies": {
		"@nan0web/db": "^1.0.0",
		"@nan0web/db-fs": "^1.0.0",
		"@nan0web/types": "^1.0.0"
	}
}
